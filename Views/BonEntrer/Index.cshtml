@model IEnumerable<StockApp.Models.TB_bonEntre>



@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered" id="myTable">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date_entre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCreer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreerPar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TB_livraison.Code_fiche)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date_entre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreerPar)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TB_livraison.Code_fiche)
            </td>
            <td>


                @Html.ActionLink("Edit", "Edit", new { id = item.Id_bon_entrestock }) |
                @*@Html.ActionLink("Details", "Details", new { id=item.Id_bon_entrestock }) |*@
                @Html.ActionLink("Delete", "Delete", new { id = item.Id_bon_entrestock })
                <a href="../LotEntrer/Index/?id=@(item.Id_bon_entrestock)" class="btn btn-info btn-sm" data-toggle="tooltip" data-placement="top"
                   title="Telecharger"> 
                    <span class="glyphicon glyphicon-info-sign"></span> Details
                </a>
            </td>
        </tr>
    }

</table>
